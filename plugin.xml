<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.ui.bindings">
		<scheme
			id="IntelliJIdeaKeymap4EclipseScheme"
			name="IntelliJ IDEA"
			parentId="org.eclipse.ui.defaultAcceleratorConfiguration">
		</scheme>

<!-- Mapping of IntelliJ IDEA DEFAULT KEYMAP: https://resources.jetbrains.com/storage/products/intellij-idea/docs/IntelliJIDEA_ReferenceCard.pdf -->

<!-- Keys are ordered as in IntelliJ IDEA: Settings > Keymap -->


		<!-- #######################################-->
		<!-- ***** START OF: 'Editor Actions' ***** -->
		
		<!-- 'Extend Selection' -->		
		<key
			sequence="Ctrl+W"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.select.enclosing">
		</key>
		
		<!-- 'Shrink Selection' -->		
		<key
			sequence="Ctrl+Shift+W"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.select.last">
		</key>
		
		<!-- TODO 'Complete Current Statement': Waiting on https://bugs.eclipse.org/bugs/show_bug.cgi?id=426743 -->
		
		
		<!-- 'Duplicate current line' -->		
		<key
			sequence="M1+D"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.copyLineDown">
		</key>
		
		<!-- 'Delete line at caret' -->		
		<key
			sequence="Ctrl+Y"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.delete.line">
		</key>
		<key
			sequence="M1+BS"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			platform="cocoa"
			commandId="org.eclipse.ui.edit.text.delete.line">
		</key>
		
		
		<!-- 'Smart line join' -->		
		<key
			sequence="Ctrl+Shift+J"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.join.lines">
		</key>
		
		<!-- 'Start new line' -->		
		<key
			sequence="Shift+ENTER"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.smartEnter">
		</key>
		
		<!-- 'Start New Line Before Current' -->		
		<key
			sequence="Ctrl+Alt+ENTER"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.smartEnterInverse">
		</key>
		
		<!-- TODO 'Select till code block end'		
		<key
			sequence="Ctrl+Shift+]"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="eclipse_command">
		</key> -->
		
		<!-- 'Move Caret to Code Block Start' -->
		<!-- Bug: Eclipse has no start or end block just matching bracket -->
		<key
			sequence="Ctrl+["
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.goto.matching.bracket">
		</key>
		
		<!-- 'Move Caret to Code Block End' -->
		<!-- Bug: Eclipse has no start or end block just matching bracket -->
		<key
			sequence="Ctrl+]"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.goto.matching.bracket">
		</key>
		
		<!-- 'Delete to word end' -->		
		<key
			sequence="Ctrl+DELETE"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.deleteNextWord">
		</key>
		
		<!-- 'Delete to word start' -->		
		<key
			sequence="Ctrl+BS"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.deletePreviousWord">
		</key>
		
		<!-- 'Collapse code block' -->		
		<key
			sequence="M1+NUMPAD_SUBTRACT"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.collapse">
		</key>
		<key
			sequence="M1+-"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.collapse">
		</key>
		
		<!-- 'Expand code block' -->		
		<key
			sequence="M1+NUMPAD_ADD"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.expand">
		</key>
		<key
			sequence="M1+="
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.expand">
		</key>
		
		<!-- 'Collapse all' -->		
		<key
			sequence="M1+M2+NUMPAD_SUBTRACT"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.collapse_all">
		</key>
		<key
			sequence="M1+M2+-"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.collapse_all">
		</key>
		
		
		<!-- 'Expand all' -->		
		<key
			sequence="M1+M2+NUMPAD_ADD"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.expand_all">
		</key>
		<key
			sequence="M1+M2+="
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.folding.expand_all">
		</key>
		
		<!-- TODO 'Close active editor tab'
		<key
			sequence="Ctrl+F4"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.file.close"> Ctrl+F4 should also close Views
		</key> -->


		<!-- 'Go to editor (from tool window)' -->		
		<key
			sequence="ESC"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.window.activateEditor">
		</key>

		<!-- ##### END OF: 'Editor Actions' ##### -->












		<!-- #########################################-->
		<!-- ***** START OF: 'Main Menu > File' ***** -->
		
		<!-- 'Settings...', 'Open Settings dialog'  -->		
		<key
			sequence="Ctrl+Alt+S"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.window.preferences">
		</key>
		
		<!-- 'Project Structure...', 'Open Project Structure dialog' -->		
		<key
			sequence="Ctrl+Alt+Shift+S"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.project.properties">
		</key>
		
		<!-- 'Save all' -->		
		<key
			sequence="M1+S"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.file.saveAll">
		</key>
		
		<!-- TODO What exactly does Synchronize do? Is it just refresh? -->
		<!-- 'Synchronize' -->		
		<key
			sequence="Ctrl+Alt+Y"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.file.refresh">
		</key>
		<!-- ##### END OF: 'Main Menu > File' ##### -->










		
		
		
		
		
		<!-- #########################################-->
		<!-- ***** START OF: 'Main Menu > Edit' ***** -->
		
		<!-- 'Select All' -->
		<key
			sequence="M1+A"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.selectAll">
		</key>
		
		<!-- 'Undo' -->		
		<key
			sequence="M1+Z"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.undo">
		</key>
		
		<!-- 'Redo' -->		
		<key
			sequence="M1+Shift+Z"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.redo">
		</key>
		
		<!-- #########################################-->
		<!-- ***** START OF: 'Main Menu > Edit > Find' ***** -->
		
		<!-- TODO Search everywhere -->
		
		
		<!-- 'Find...' -->		
		<key
			sequence="M1+F"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.findReplace">
		</key>
		<key
			sequence="Alt+F3"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.findReplace">
		</key>
		
		<!-- 'Replace' -->		
		<key
			sequence="M1+R"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.findReplace">
		</key>
		
		
		<!-- 'Find Next' -->		
		<key
			sequence="F3"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.findNext">
		</key>
		<key
			sequence="Ctrl+L"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.findNext">
		</key>
		
		<!-- 'Find Previous' -->		
		<key
			sequence="Shift+F3"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.findPrevious">
		</key>
		<key
			sequence="Ctrl+Shift+L"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.findPrevious">
		</key>
		
		<!-- 'Find in path' -->		
		<key
			sequence="Ctrl+Shift+F"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.search.ui.openFileSearchPage">
		</key>
		
		<!-- 'Replace in path' -->
		<key
			sequence="Ctrl+Shift+R"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.search.ui.openFileSearchPage">
		</key>
		
		<!-- TODO multiple cursors required? Select next occurrence, Select all occurrences, Unselect occurrence -->
		
		
		<!-- 'Find usages' -->
		<key
			sequence="M3+F7"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.search.references.in.workspace">
		</key>
		
		<!-- 'Highlight usages in file' -->
		<key
			sequence="M1+M2+F7"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.search.occurrences.in.file.quickMenu">
		</key>
		
		<!-- 'Show usages' -->
		<key
			sequence="M1+M3+F7"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.search.references.in.workspace">
		</key>
		
		
		
		
		<!-- ##### END OF: 'Main Menu > Edit > Find' ##### -->
		
		
		<!-- ##### END OF: 'Main Menu > Edit' ##### -->











		<!-- #########################################-->
		<!-- ***** START OF: 'Main Menu > View' ***** -->

		<!-- TODO 'Quick switch current scheme' Ctrl + BackQuote (`) --> 

		<!-- 'Parameter Info' -->		
		<key
			sequence="CTRL+P"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.edit.text.contentAssist.contextInformation">
		</key>
		
		<!-- TODO It would be better to call JavadocHover, but how can I do this? -->
		<!-- 'Quick Documentation' -->
		<key
			sequence="Ctrl+Q"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.views.showView">
			<parameter
                id="org.eclipse.ui.views.showView.viewId"
                value="org.eclipse.jdt.ui.JavadocView">
          </parameter>
		</key>
		
		<!-- 'External Documentation' -->		
		<key
			sequence="SHIFT+F1"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.open.external.javadoc">
		</key>
		
		
		
		<!-- 'Show descriptions of error at caret' -->		
		<!-- TODO Bug in Eclipse, it shows the Eclipse help instead of the dialog. -->
		<key
			sequence="Ctrl+F1"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.edit.text.showInformation">
		</key>
		
		<!-- TODO It would be better to call JavaSourceHover, but how can I do this? -->
		<!-- 'Quick definition' -->		
		<key
			sequence="M1+SHIFT+I"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.views.showView">
			<parameter
                id="org.eclipse.ui.views.showView.viewId"
                value="org.eclipse.jdt.ui.SourceView">
          </parameter>
		</key>
		
		<!-- ##### END OF: 'Main Menu > View' ##### -->















		<!-- #########################################-->
		<!-- ***** START OF: 'Main Menu > Navigate' ***** -->
		
		<!-- 'Go to class' -->		
		<key
			sequence="Ctrl+N"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.contexts.window"
			commandId="org.eclipse.jdt.ui.navigate.open.type">
		</key>
		
		<!-- 'Go to file' -->		
		<key
			sequence="Ctrl+Shift+N"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.contexts.window"
			commandId="org.eclipse.ui.navigate.openResource">
		</key>
		
		<!-- 'Go to line' -->		
		<key
			sequence="Ctrl+G"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.goto.line">
		</key>
		
		<!-- 'Navigate forward ' -->		
		<key
			sequence="CTRL+ALT+ARROW_RIGHT"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.navigate.forwardHistory">
		</key>
		
		<!-- 'Navigate back ' -->		
		<key
			sequence="CTRL+ALT+ARROW_LEFT"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.navigate.backwardHistory">
		</key>
		
		<!-- 'Navigate to last edit location' -->		
		<key
			sequence="Ctrl+Shift+BS"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.contexts.window"
			commandId="org.eclipse.ui.edit.text.gotoLastEditPosition">
		</key>
		
		<!-- 'Select current file or symbol in any view' -->		
		<key
			sequence="ALT+F1"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.navigate.showInQuickMenu">
		</key>
		
		<!-- 'Go to declaration' -->		
		<key
			sequence="Ctrl+B"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.open.editor">
		</key>
		
		<!-- 'Go to implementation' -->		
		<key
			sequence="Ctrl+Alt+B"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.open.implementation">
		</key>
		
		<!-- TODO 'Go to type declaration' -->
		
		<!-- 'Go to super-method / super-class' -->		
		<key
			sequence="Ctrl+U"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.open.super.implementation">
		</key>
		
		<!-- 'Go to previous method' -->		
		<key
			sequence="ALT+ARROW_UP"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.goto.previous.member">
		</key>
		<key
			sequence="ALT+ARROW_UP"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			platform="cocoa"
			commandId="">
		</key>
		<key
			sequence="M4+ARROW_UP"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			platform="cocoa"
			commandId="org.eclipse.jdt.ui.edit.text.java.goto.previous.member">
		</key>
		
		<!-- 'Go to next method' -->		
		<key
			sequence="ALT+ARROW_DOWN"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.goto.next.member">
		</key>
		<key
			sequence="ALT+ARROW_DOWN"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			platform="cocoa"
			commandId="" />
		<key
			sequence="M4+ARROW_DOWN"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			platform="cocoa"
			commandId="org.eclipse.jdt.ui.edit.text.java.goto.next.member">
		</key>
		
		<!-- 'File structure popup' -->		
		<key
			sequence="Ctrl+F12"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.show.outline">
		</key>
		
		<!-- 'Type hierarchy' -->		
		<key
			sequence="Ctrl+H"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.open.type.hierarchy">
		</key>
		
		<!-- 'TODO Method hierarchy' -->		
		
		
		<!-- 'Call hierarchy' -->
		<key
			sequence="Ctrl+Alt+H"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.open.call.hierarchy">
		</key>
		
		<!-- 'Jump to Navigation Bar' -->		
		<key
			sequence="ALT+HOME"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.breadcrumbEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.gotoBreadcrumb">
		</key>
		
		<!-- 'Add Bookmark' -->		
		<key
			sequence="F11"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.edit.addBookmark">
		</key>
		
		<!-- 'Toggle bookmark with mnemonic' -->		
		<key
			sequence="Ctrl+F11"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.edit.addBookmark">
		</key>
		
		<!-- 'Show bookmarks' -->		
		<key
			sequence="Shift+F11"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.ui.views.showView">
			<parameter
                id="org.eclipse.ui.views.showView.viewId"
                value="org.eclipse.ui.views.BookmarkView">
          </parameter>
		</key>
		
		<!-- ##### END OF: 'Main Menu > Navigate' ##### -->



		






		
		<!-- #########################################-->
		<!-- ***** START OF: 'Main Menu > Code' ***** -->
		
		<!-- 'Surround with Live Template' -->		
		<key
			sequence="Ctrl+Alt+J"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.surround.with.quickMenu">
		</key>
		
		<!-- 'Insert Live Template' -->		
		<key
			sequence="Ctrl+J"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.specific_content_assist.command">
			<parameter
                id="org.eclipse.jdt.ui.specific_content_assist.category_id"
                value="org.eclipse.jdt.ui.templateProposalCategory">
          </parameter>
		</key>
		
		<!-- 'Generate code' -->		
		<key
			sequence="ALT+INSERT"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.jdt.ui.javaEditorScope"
			commandId="org.eclipse.jdt.ui.edit.text.java.source.quickMenu">
		</key>
		
		<!-- 'Code Completion Basic' -->		
		<key
			sequence="CTRL+SPACE"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId="org.eclipse.ui.textEditorScope"
			commandId="org.eclipse.ui.edit.text.contentAssist.proposals">
		</key> <!-- Command+SPACE+* is reserved on carbon, hardcode to CTRL -->
		
		
		<!-- 'Reformat code' -->		
		<key
			sequence="Ctrl+Alt+L"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.format">
		</key>

		<!-- 'Organize Imports' -->		
		<key
			sequence="Ctrl+Alt+O"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.organize.imports">
		</key>
		
		<!-- 'Auto indent' -->		
		<key
			sequence="Ctrl+Alt+I"
			schemeId="IntelliJIdeaKeymap4EclipseScheme"
			contextId=""
			commandId="org.eclipse.jdt.ui.edit.text.java.indent">
		</key>
		
		<!-- 'Comment / uncomment with line comment ' -->		
		<key
			sequence="Ctrl+/"
